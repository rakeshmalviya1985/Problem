
#include <iostream>
#include <vector>
#include <sstream>
#include <string>
using namespace std; 

void shiftZeroAtend(int arr[], int size)
{
    int j = size-1;
    int tmp;
    int i = 0;
    while(i<size && i<j)
    {
        if(arr[j] != 0 && arr[i] == 0)
        {
            tmp = arr[i];
            arr[i] = arr[j];
            arr[j] = tmp;
           i++;
           j--;
        }
        else if(arr[j] != 0 && arr[i] != 0)
           i++;
        else
           j--;
    }
    
    for(int i = 0;i<size;i++)
       cout<<" "<<arr[i];
}

int main() 
{ 

    int arr[] = {1, 9, 8, 4, 0, 0, 2, 7, 0, 6, 0, 9}; 
    int n = sizeof(arr) / sizeof(arr[0]);
    shiftZeroAtend(arr,n);
	return 0; 
} 

